<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.bawei.redchild">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:name=".RedChildApplication"
        android:allowBackup="true"
        android:icon="@mipmap/icon_info"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".HomeActivity"/>
        <activity android:name=".me.me.Login_act"/>
        <activity android:name=".baby_info.BabyInfoActivity"/>
        <activity android:name=".me.me.Register_act">
        </activity>
        <activity android:name=".shoppingCart.Main2Activity">
        </activity>
    </application>

</manifest>